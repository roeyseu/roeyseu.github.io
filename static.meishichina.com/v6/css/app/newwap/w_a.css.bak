.blist_c1 li div a span img{background:0 0;width:100%}


/* 正方图 1行2 */
.blist_c3{padding:0 0 0 10px}
.blist_c3 li{width:50%;float:left;margin:0 0 15px 0;}
.blist_c3 li div{padding:0 10px 0 0}
.blist_c3 li div a{display:block;font-size:14px;text-align:center;}
.blist_c3 li div a span{display:block;padding:0 0 100% 0;height:0;overflow:hidden;margin:0 0 5px 0;background:url(../../../img/wap_a/xiaolian.png) no-repeat scroll center center/16px 15px #fff}
.blist_c3 li div a span img{background:0 0;width:100%}
