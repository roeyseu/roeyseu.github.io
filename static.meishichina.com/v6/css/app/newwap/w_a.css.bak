/* 首页list正方图 60×60 （话题）*/
.alist_c3{padding:0 0 0 10px;}
.alist_c3 li a{display:block;width:100%;height:60px;padding:10px 0;border-bottom:1px solid #ddd;}
.alist_c3 li:nth-child(1) a{border-top:1px solid #ddd;}
.alist_c3 .pic{float:left;width:60px;height:60px;}
